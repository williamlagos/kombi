{
    "name": "mars-client",
    "version": "0.6.0",
    "author": "Quito Labs",
    "homepage": "http://quitolabs.org/",
    "repository": "http://efforia.io",
    "description": "Awesome Mars Framework Application!",
    "license": "MIT",
    "keywords": [
        "mean",
        "ionic",
        "awesome"
    ],
    "main": "pwa/app.js",
    "config": {
        "ionic_tslint": "./src/**/*.ts",
        "ionic_bundler": "webpack",
        "ionic_webpack": "webpack.config.js",
        "ionic_source_map": "source-map",
        "ionic_generate_source_map": false
    },
    "sideEffects": false,
    "scripts": {
        "start": "node pwa/app.js",
        "dev": "npm run serve",
        "prepare:package": "node .bin/prepare/package",
        "clean": "ionic-app-scripts clean",
        "build": "npm run prepare:package && node --max-old-space-size=8192 ./node_modules/@ionic/app-scripts/bin/ionic-app-scripts.js build",
        "serve": "npm run prepare:package && ionic-app-scripts serve",
        "webpack": "ionic-app-scripts webpack",
        "build:prod": "npm run prepare:package && node --max-old-space-size=8192 ./node_modules/@ionic/app-scripts/bin/ionic-app-scripts.js build --turbo --aot --minifyjs --minifycss --release",
        "bundle-stats": "npm run build:prod --aot --minifyjs --minifycss --release -- --stats-json && webpack-bundle-analyzer ./www/build/stats.json",
        "pwa": "node pwa/app.js"
    },
    "dependencies": {
        "@angular/animations": "6.0.7",
        "@angular/common": "6.0.7",
        "@angular/compiler": "6.0.7",
        "@angular/compiler-cli": "6.0.7",
        "@angular/core": "6.0.7",
        "@angular/forms": "6.0.7",
        "@angular/http": "6.0.7",
        "@angular/platform-browser": "6.0.7",
        "@angular/platform-browser-dynamic": "6.0.7",
        "@fnando/cnpj": "^0.1.0",
        "@fnando/cpf": "^0.1.0",
        "@ionic/app-scripts": "^3.2.0",
        "@ionic/cli-utils": "1.9.0",
        "@types/socket.io-client": "1.4.29",
        "ajv": "^6.5.3",
        "ajv-keywords": "^3.2.0",
        "angular2-http-file-upload": "1.0.6",
        "autocompleter": "1.0.30",
        "axios": "^0.12.0",
        "body-parser": "^1.18.3",
        "chalk": "2.3.0",
        "chart.js": "2.7.2",
        "collect.js": "4.0.17",
        "color": "^3.0.0",
        "colors": "^1.3.2",
        "compression": "^1.7.3",
        "compression-webpack-plugin": "1.0.1",
        "cookie-parser": "^1.4.3",
        "cordova-android": "^7.0.0",
        "cordova-browser": "^5.0.3",
        "cordova-ios": "^4.5.2",
        "cordova-plugin-compat": "^1.2.0",
        "cordova-plugin-geolocation": "^3.0.0",
        "cordova-plugin-inappbrowser": "^1.7.2",
        "cordova-plugin-splashscreen": "^4.1.0",
        "cordova-plugin-statusbar": "^1.0.1",
        "cordova-plugin-whitelist": "^1.3.3",
        "cordova-update-config": "^1.0.2",
        "cors": "^2.8.4",
        "errorhandler": "^1.5.0",
        "express": "^4.16.3",
        "express-static-gzip": "^1.1.3",
        "extract-text-webpack-plugin": "3.0.2",
        "flatpickr": "^4.5.2",
        "geolocator": "2.1.1",
        "greenlock-express": "^2.4.6",
        "handlebars": "4.0.11",
        "hellojs": "1.16.1",
        "intl": "1.2.5",
        "intro.js": "2.9.3",
        "ionic": "3.20.0",
        "ionic-angular": "3.9.2",
        "ionic-plugin-keyboard": "^2.2.1",
        "ionic2-rating": "1.2.2",
        "ionicons": "3.0.0",
        "ip": "1.1.5",
        "jslint": "0.11.0",
        "klaw-sync": "^6.0.0",
        "module-alias": "^2.1.0",
        "moment-mini": "2.22.1",
