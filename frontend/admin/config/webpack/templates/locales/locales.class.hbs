import { Injectable } from "@angular/core";
import { AppTranslations } from "../typings/global/translations/index.d.ts";
    
@Injectable()
export class AppLocales {
      {{#each languages}}
      private {{this.code}}: any = {
      {{#each this.locales}}
            "{{this.key}}": `{{{this.value}}}`,
      {{/each}}
      };
      {{/each}}

      public load(): AppTranslations {
            return this["pt" || navigator.language];
      }
}