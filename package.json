    "ngzip": "^1.1.3",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "shelljs": "^0.8.2",
    "swagger-es6": "^1.0.0",
    "typescript": "2.7.2",
    "uglify-js": "^3.4.9",
    "watch": "1.0.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "2.9.1",
    "yargs": "^12.0.2"
  }
