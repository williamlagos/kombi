  "config": {},
  "main": ".bin/index",
  "scripts": {
    "postinstall": "node .bin/dependencies",
    "babel": "babel",
    "uglify-js": "uglify-js",
    "gzip-cli": "gzip-cli",
    "universal_scripts": "|-------------- general application scripts",
    "prepare:app": "node .bin/app/prepare",
    "admin_scripts": "|-------------- admin app scripts",
    "admin": "npm run prepare:app --app=admin & cd dashboard && npm run serve",
    "admin:export": "npm run prepare:app --app=admin & node .bin/app/export/web --app=admin",
    "customer_scripts": "|-------------- customer application scripts",
    "customer": "npm run prepare:app --app=customer & cd customer && npm run serve",
    "customer:export": "npm run prepare:app --app=customer & node .bin/app/export/web --app=customer",
    "driver_scripts": "|-------------- driver application scripts",
    "driver": "npm run prepare:app --app=driver & cd driver && npm run serve",
    "driver:export": "npm run prepare:app --app=driver & node .bin/app/export/web --app=driver"
  },
  "dependencies": {
    "@ionic/app-scripts": "^3.2.0",
    "brotli": "^1.3.2",
    "klaw-sync": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
