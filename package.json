{
  "name": "mars-fullstack",
  "version": "0.6.0",
  "author": "Quito Labs",
  "homepage": "http://quitolabs.org/",
  "description": "Awesome Mars Framework Application!",
  "license": "MIT",
  "keywords": [
    "mars",
    "mean",
    "ionic",
    "angular",
    "express",
    "awesome"
  ],
  "config": {},
  "main": ".bin/index",
  "scripts": {
    "postinstall": "node .bin/dependencies",
    "prepare-app": "node .bin/prepare",
    "start": "node .bin/index",
    "database": "mongod",
    "app_tasks": "------------ Client Application Tasks",
    "app": "npm run app:prepare && cd frontend/app && npm run dev",
    "app:prepare": "npm  --app=frontend/app run prepare-app",
    "app:deploy": "npm run app:prepare && cd frontend/app && node .bin/deploy",
    "app:export:android": "npm run app:prepare && cd frontend/app && npm run build:prod && node .bin/prepare/cordova && node .bin/android/export",
    "app:generate:api": "node frontend/app/.bin/generate/api",
    "app:pwa": "cd frontend/app && npm run pwa",
    "landing_tasks": "------------ Landing Page Tasks",
    "landing": "npm run landing:prepare && cd frontend/landing && npm run dev",
    "landing:prepare": "npm --app=frontend/landing run prepare-app ",
    "landing:deploy": "npm run landing:prepare && cd frontend/landing && node .bin/deploy",
    "landing:generate:favicons": "npm run landing:prepare && node frontend/landing/.bin/generate/favicons",
    "server_tasks": "------------ Server Application Tasks",
    "server": "npm run server:prepare && cd backend/server && npm run dev",
    "server:prepare": "npm --app=backend/server run prepare-app",
    "server:deploy": "npm run server:prepare && cd backend/server && node .bin/deploy",
    "socket_tasks": "------------ Socket Application Tasks",
    "socket": "npm run socket:prepare && cd backend/socket && npm run dev",
    "socket:prepare": "npm --app=backend/socket run prepare-app",
    "socket:deploy": "npm run socket:prepare && cd backend/socket && node .bin/deploy"
  },
  "dependencies": {
    "babel-core": "7.0.0-alpha.19",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "buffer-split": "1.0.0",
    "chart.js": "^2.7.2",
    "color": "^3.0.0",
    "colors": "^1.3.2",
    "favicons": "^5.2.0",
    "fs-extra": "^7.0.0",
    "js-base64": "^2.4.5",
    "mammoth": "^1.4.6",
    "module-alias": "^2.1.0",
    "npm-check": "^5.9.0",
    "nps": "^5.9.3",
    "openode": "^1.5.9",
    "shelljs": "^0.8.2",
    "striptags": "3.1.1",
    "swagger-es6": "^1.0.0",
    "titlecase": "1.1.2",
    "watch": "^1.0.2",
    "word-extractor": "0.1.4",
    "xlsx-to-json": "0.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  }
}